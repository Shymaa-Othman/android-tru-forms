{
  "title": "Other Claim",
  "type": "object",
  "properties": {
    "claim_information": {
      "type": "object",
      "properties": {
        "claim_description": {
          "type": "string"
        },
        "date_of_loss": {
          "type": "string",
          "format": "date"
        },
        "your_damage_photos": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "photo": {
                "type": "string",
                "format": "photo"
              }
            }
          }
        }
      },
      "required": [
        "claim_description",
        "date_of_loss"
      ]
    },
    "witness_information": {
      "type": "array",
      "items": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string"
          },
          "email": {
            "type": "string",
            "format": "email"
          },
          "home_phone_number": {
            "type": "string",
            "format": "tel"
          },
          "cell_phone_number": {
            "type": "string",
            "format": "tel"
          }
        }
      }
    }
  },
  "required": []
}